#ifndef INC_SCHEDULE_H_
#define INC_SCHEDULE_H_

#include <stdint.h>

typedef struct{
	void (*pTask)(void);
	uint32_t Delay;
	uint32_t Period;
	uint8_t RunMe;
	uint32_t TaskID;
}sTask;

#define SCH_MAX_TASKS	40
#endif /* INC_SCHEDULE_H_ */
